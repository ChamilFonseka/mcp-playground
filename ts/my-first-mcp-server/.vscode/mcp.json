{
	"servers": {
		"my-mcp-server": {
			"type": "stdio",
			"command": "ns",
			"args": [
				"&&",
				"node",
				"build/index.js",
			],
			"cwd": "${workspaceFolder}",
			"dev": {
				"watch": "build/**/*.js",
				"debug": {
					"type": "node"
				}
			},
			"env": {
				"NAME": "${input:name}",

			}
		}
	},
	"inputs": [
		{
			"type": "promptString",
			"id": "name",
			"description": "Name to say hello to",
			"password": false
		},
	]
}